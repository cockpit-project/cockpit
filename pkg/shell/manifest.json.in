{
    "version": "@VERSION@",
    "requires": {
        "cockpit": "134.x"
    },
    "locales": {
        "ca-es": "català",
        "cs-cz": "čeština",
        "de-de": "Deutsch",
        "en-us": "English",
        "es-es": "español",
        "fi-fi": "suomi",
        "fr-fr": "français",
        "it-it": "italiano",
        "ja-jp": "日本語",
        "ko-kr": "한국어",
        "pl-pl": "polski",
        "pt-br": "português (Brasil)",
        "ru-ru": "русский",
        "sv-se": "svenska",
        "uk-ua": "Українська",
        "zh-cn": "中文（中国）",
        "zh-tw": "中文（台灣）"
    },
    "bridges": [
        {
            "match": {
                "superuser": null
            },
            "spawn": [
                "@PKEXEC@",
                "--disable-internal-agent",
                "cockpit-bridge",
                "--privileged"
            ]
        },
        {
            "match": {
                "superuser": null
            },
            "environ": [
                "SUDO_ASKPASS=@libexecdir@/cockpit-askpass"
            ],
            "spawn": [
                "@SUDO@",
                "-A",
                "cockpit-bridge",
                "--privileged"
            ]
        }
    ]
}
