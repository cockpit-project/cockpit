{
    "version": "@VERSION@",
    "name": "updates",
    "priority": 0,
    "requires": {
        "cockpit": "186"
    },

    "tools": {
        "index": {
            "label": "Software Updates",
            "keywords": [
                {
                    "matches": ["package", "packagekit", "dnf", "yum", "apt-get", "security"]
                }
            ]
        }
    },

    "preload": [ "index" ],

    "content-security-policy": "default-src 'self'; img-src 'self' data:; font-src 'self' data:;"
}
