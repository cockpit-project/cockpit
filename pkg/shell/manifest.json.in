{
    "version": "@VERSION@",
    "requires": {
	"cockpit": "134.x"
    },

    "locales": {
        "ca-es": "català",
        "da-dk": "dansk",
        "de-de": "Deutsch",
        "en-us": "English",
        "es-es": "español",
        "fr-fr": "français",
        "ja-jp": "日本語",
        "pl-pl": "polski",
        "pt-br": "português (Brasil)",
        "tr-tr": "Türkçe",
        "uk-ua": "Українська",
        "zh-cn": "中文（中国）"
    },

    "bridges": [
        {
            "match": { "superuser": null },
            "spawn": [ "@PKEXEC@", "--disable-internal-agent", "cockpit-bridge", "--privileged" ]
        },
        {
            "match": { "superuser": null },
            "environ": [ "SUDO_ASKPASS=@libexecdir@/cockpit-askpass" ],
            "spawn": [ "@SUDO@", "-A", "cockpit-bridge", "--privileged" ]
        }
    ]
}
