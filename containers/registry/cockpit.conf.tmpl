[WebService]
Shell = /shell/simple.html
LoginTitle = Atomic Registry

{{if .is_openshift }}
{{else}}
[Negotiate]
Action = spawn-login-with-decoded
Command = {{.login_command}}
{{end}}

[Basic]
Action = spawn-login-with-decoded
Command = {{.login_command}}

{{if .oauth_url }}
[Bearer]
Action = spawn-login-with-header
Command = {{.login_command}}

[OAuth]
Url = {{.oauth_url}}
{{end}}
