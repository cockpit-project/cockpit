[WebService]
Shell = /shell/simple.html
{{if .is_openshift }}
LoginTitle = Cockpit Openshift
{{else}}
LoginTitle = Cockpit Kubernetes

[Negotiate]
Action = spawn-login-with-decoded
Command = {{.login_command}}
{{end}}

[Basic]
Action = spawn-login-with-decoded
Command = {{.login_command}}

{{if .oauth_url }}
[Bearer]
Action = spawn-login-with-header
Command = {{.login_command}}

[OAuth]
Url = {{.oauth_url}}
{{end}}
