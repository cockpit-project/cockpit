{
   "kind":"List",
   "apiVersion":"v1",
   "items":[
      {
         "kind":"ReplicationController",
         "apiVersion":"v1",
         "metadata":{
            "name":"mock",
            "labels":{
               "name":"mock"
            }
         },
         "spec":{
            "replicas":1,
            "selector":{
               "name":"mock"
            },
            "template":{
               "metadata":{
                  "labels":{
                     "name":"mock"
                  }
               },
               "spec":{
                  "containers":[{
                    "name": "mock-container",
                    "image": "busybox:buildroot-2014.02",
                    "command": [ "/bin/sh", "-c", "for x in $(seq 1 1000); do echo 'HelloMessage.' >&2; sleep 1; done" ],
                    "ports":[{
                        "containerPort":9949,
                        "protocol":"TCP"
                     }]
                  }]
               }
            }
         }
      },
      {
         "kind":"Service",
         "apiVersion":"v1",
         "metadata":{
            "name":"mock",
            "labels":{
               "name":"mock"
            }
         },
         "spec":{
             "ports": [{
                 "name": "",
                 "protocol": "TCP",
                 "port": 99,
                 "targetPort": 9949
             }],
            "selector":{
               "name":"mock"
            }
         }
      }
   ]
}
