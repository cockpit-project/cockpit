adjust+:
  when: revdeps == yes
  enabled: false

discover:
    how: fmf
execute:
    how: tmt

# Let's handle them upstream only, don't break Fedora/RHEL reverse dependency gating
environment:
    TEST_AUDIT_NO_SELINUX: 1

/optional:
    summary: Run tests for optional packages
    discover+:
        test: /test/browser/optional
