{
    "apiVersion": "v1",
    "kind": "Pod",
    "metadata": {
        "name": "tiny-nginx",
        "labels": {
            "name": "tiny-nginx"
        }
    },
    "spec": {
        "containers": [
            {
                "name": "tiny-nginx",
                "image": "fedora/nginx",
                "ports": [
                    {
                        "containerPort": 80,
                        "protocol": "TCP"
                    }
                ]
            }
        ]
    }
}

