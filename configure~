ac_subst_files=''
ac_user_opts='
enable_option_checking
enable_silent_rules
enable_dependency_tracking
enable_largefile
enable_prefix_only
enable_selinux_policy
enable_polkit
enable_ssh
with_pamdir
enable_pcp
