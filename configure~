printf "%s\n" "#define _DEBUG 1" >>confdefs.h

  CFLAGS="$CFLAGS -g"
fi
debugdir='${prefix}/src/debug'
if test "$enable_debug" = "yes"; then
  debug_status="yes"
  debugdir=
  CFLAGS="$CFLAGS -O0"
  NODE_ENV="development"
elif test "$enable_debug" = "no"; then
  debug_status="no"
  CFLAGS="$CFLAGS -O2"
  NODE_ENV="production"
else
  debug_status="default"
  NODE_ENV="${NODE_ENV:-production}"
fi
 if test "$enable_debug" = "yes"; then
  WITH_DEBUG_TRUE=
  WITH_DEBUG_FALSE='#'
else
  WITH_DEBUG_TRUE='#'
  WITH_DEBUG_FALSE=
