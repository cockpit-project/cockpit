[Unit]
Description=Cockpit Web Service
Documentation=man:cockpit-ws(8)
Requires=cockpit.socket

[Service]
ExecStartPre=@sbindir@/remotectl certificate --ensure --user=@cockpit_user@ --group=@cockpit_group@ --selinux-type=@selinux_config_type@
ExecStart=@libexecdir@/cockpit-ws
PermissionsStartOnly=true
User=@cockpit_user@
Group=@cockpit_group@
